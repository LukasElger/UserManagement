- if !user_signed_in?
  .row.justify-content-center
    .col-sm-2.align_center.index
      %h1.header= t "views.application.index.welcome"
      = button_to I18n.t("actions.login"), login_path, method: :get, class: "btn btn-primary"
- else
  - if current_user.admin?
    = render "application/partials/admin_dashboard"
  - else
    = render "application/partials/not_admin_dashboard"
